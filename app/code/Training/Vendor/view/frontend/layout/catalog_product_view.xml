<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="product.attributes" remove="true">
            <!-- <action method="setTemplate">
                <argument name="template" xsi:type="string">Training_Vendor::product/view/attributes.phtml</argument>
            </action> -->
        </referenceBlock>
        <referenceBlock name="product.info.details">
                <referenceBlock name="product.info.attributes" remove="true"/>
                <block class="Magento\Catalog\Block\Product\View\Attributes" name="product.info.attributescustom" template="Training_Vendor::product/view/attributes.phtml" group="detailed_info">
                    <arguments>
                        <argument name="at_call" xsi:type="string">getAttributeText</argument>
                        <argument name="at_code" xsi:type="array">
                            <item name="item1" xsi:type="string">my_multiselect_attribute</item>
                            <item name="item2" xsi:type="string">priority</item>
                        </argument>
                        <argument name="css_class" xsi:type="string">attributes</argument>
                        <argument name="at_label" xsi:type="string">none</argument>
                        <argument name="title" translate="true" xsi:type="string">Attributes</argument>
                    </arguments>
                </block>
            </referenceBlock>
    </body>
</page>
